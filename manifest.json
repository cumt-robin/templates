{
    "version": "1.1.0",
    "list": [
        {
            "name": "browser-ts-lib",
            "title": "【基于ts的浏览器库】模板",
            "description": "ts library template for browser",
            "runtimeVars": ["currentYear"],
            "globalVars": [
                {
                    "name": "name",
                    "type": "string",
                    "description": "项目名称",
                    "default": "browser-ts-lib"
                },
                {
                    "name": "author",
                    "type": "string",
                    "description": "项目作者",
                    "default": "程序员白彬"
                },
                {
                    "name": "description",
                    "type": "string",
                    "description": "项目描述",
                    "default": "ts library template for browser"
                }
            ],
            "templateFiles": [
                {
                    "path": "package.json",
                    "useGlobalVars": ["name", "author", "description"]
                },
                {
                    "path": "rollup.config.js",
                    "vars": [
                        {
                            "name": "libGlobalName",
                            "type": "string",
                            "description": "库对应的window全局变量名",
                            "default": "GlobalLib"
                        }
                    ]
                },
                {
                    "path": "README.md",
                    "useGlobalVars": ["name"]
                },
                {
                    "path": "LICENSE",
                    "useGlobalVars": ["author"],
                    "useRuntimeVars": ["currentYear"]
                }
            ]
        },
        {
            "name": "vite-vue3-ts-pinia-antdv",
            "title": "【基于vite + vue3 + ts + pinia + ant-design-vue】模板",
            "description": "fast development template based on vite, vue3, ts, pinia, ant-design-vue",
            "runtimeVars": ["currentYear"],
            "globalVars": [
                {
                    "name": "name",
                    "type": "string",
                    "description": "项目名称",
                    "default": "vite-vue3-ts-pinia-antdv"
                },
                {
                    "name": "author",
                    "type": "string",
                    "description": "项目作者",
                    "default": "程序员白彬"
                },
                {
                    "name": "description",
                    "type": "string",
                    "description": "项目描述",
                    "default": "fast development template based on vite, vue3, ts, pinia, ant-design-vue"
                }
            ],
            "templateFiles": [
                {
                    "path": "package.json",
                    "useGlobalVars": ["name", "author", "description"]
                },
                {
                    "path": "LICENSE",
                    "useGlobalVars": ["author"],
                    "useRuntimeVars": ["currentYear"]
                }
            ]
        }
    ]
}
